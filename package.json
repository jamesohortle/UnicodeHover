		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jamesohortle/UnicodeHover"

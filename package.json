{
	"name": "unicodehover",
	"displayName": "UnicodeHover",
	"description": "Hover over Unicode escapes to see a glyph of the character.",
	"publisher": "jamesohortle",
	"icon": "img/icon.png",
	"version": "0.3.0",
	"license": "MIT",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:python",
		"onLanguage:javascript",
		"onLanguage:typescript",
		"onLanguage:latex",
		"onLanguage:java",
		"onLanguage:html",
		"onLanguage:css",
		"onLanguage:haskell"
	],
	"main": "out/extension",
	"contributes": {},
	"scripts": {
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "npm run compile",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/react": "^17.0.6",
		"@types/vscode": "^1.30.0",
		"glob": "^7.1.5",
		"mocha": "^8.4.0",
		"tslint": "^6.1.3",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jamesohortle/UnicodeHover"
	},
	"dependencies": {
		"lodash": ">=4.17.21",
		"minimist": ">=0.2.1"
	}
}
